{
  "name": "quasar-ts-starter",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Egyel",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit && eslint --ext .ts,.js,.vue --ignore-path .gitignore src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "i18n": "vue-i18n-extract -v \"./src/**/*.?(ts|js|vue)\" -l \"./src/i18n/*.?(json)\"",
    "i18n:extract": "vue-i18n-extract -v \"./src/**/*.?(ts|js|vue)\" -l \"./src/i18n/*.?(json)\" -o ./src/i18n/output.json",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.6.4",
    "axios": "^0.18.1",
    "quasar": "^1.9.15",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.0.0",
    "vue-property-decorator": "^8.4.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.7.1",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "@types/node": "10.17.15",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^6.1.2",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "typescript": "^3.8.3",
    "vue-i18n-extract": "^1.1.1",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}